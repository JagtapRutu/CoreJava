	public static void main(String[] args) {
		// IF STATEMENT --> IT IS USED WHEN WE HAVE SINGLE CONDITION TO CHECK
	 int a=20; int b=10;
	 if (a>b) {
		 System.out.println(a+"is grrater than"+b);
		 
	 }
	 scanner sc = new scanner(System.in);
	}

}
