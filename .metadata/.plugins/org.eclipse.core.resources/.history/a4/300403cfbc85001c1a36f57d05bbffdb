package com.arrays;

public class OneDimArray {

	public static void main(String[] args) {
	//STORING VALUES IN ARRAY 
		int[] a1= {10,20,30,40,50};
	
		for(int i=0 ;i<a1.length;i++) {
		System.out.println(a1[i]);
		
	}
		int[] a2=new int[5];
		a2[0]=10;
		a2[1]=20;
		a2[2]=30;
		a2[3]=40;
		a2[4]=50;
		for(int i:a2) {
			System.out.println(i);
			
			}
		for(int a:a1) {
			System.out.println(a);
			
		}
		
				
		}

	}


