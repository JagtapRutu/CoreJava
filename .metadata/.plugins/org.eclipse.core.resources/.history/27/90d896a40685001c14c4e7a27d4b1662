package controlstatement;
 
public class continueStatement {
	public static void main(String[] args) { 
		//continue is used to skip the current iteration of the loop
		
		String a =" ";
		int j = 1;
		String b = "  ";
		for(int id=1;id<=10;10++){
		}
			if(id == 5) {
				continue;
				a=a+" "+id;
				System.out.println("student 5 is found" +a);
				  while(j<11) {
if(j==5) {
	j++;
}
continue;
b=b+"  +j" ;
System.out.println("student 5 is found: "+b);

}
		}
	}

}
